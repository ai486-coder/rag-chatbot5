{
  "version": 2,
  "builds": [
    {
      "src": "CHATBOT/main.py",
      "use": "@vercel/python"
    },
    {
      "src": "apps/web/next.config.js",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "CHATBOT/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "apps/web/$1"
    }
  ]
}
